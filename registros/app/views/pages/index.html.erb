<h1>Pages#index</h1>
<p>Find me in app/views/pages/index.html.erb</p>

<%= form_with(url: 'crear_pages') do |form| %>
  
  <div>
    <%= form.label :nombre %>
    <%= form.text_field :nombre %>
 </div>

 <div>
  <%= form.label :apellido %>
  <%= form.text_field :apellido %>
</div>

<div>
  <%= form.label :correo %>
  <%= form.email_field :correo %>
</div>

<div>
  <%= form.label :password %>
  <%= form.password_field :password %>
</div>

<div>
  <%= form.label :color_favorito %>
  <%= form.color_field :color_favorito %>
</div>

  <%= form.submit "Registrar" %>
<% end %>

